#User function Template for python3
class Solution:

	def getMoreAndLess(self,arr, n, x):
		# code here
		small=0
		large=0
		for i in range(n):
		    if arr[i]==x:
		        small+=1
		        large+=1
		        
		    if arr[i]<x:
		        small+=1
		    elif arr[i]>x:
		        large+=1
		
		return small,large
